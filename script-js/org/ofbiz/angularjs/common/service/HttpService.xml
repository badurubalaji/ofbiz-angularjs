<?xml version="1.0" encoding="UTF-8"?>
<javascript-class name="HttpService" package="org.ofbiz.angularjs.common.service">
    <javascript-method name="get">
        <attributes>
            <attribute name="uri" type="String" mode="IN"/>
            <attribute name="handler" type="Object" mode="IN"/>
        </attributes>
        <body>
            var onSuccess = handler.onSuccess;
            $http.get('jsonFiles/largeLoad.json').success(handler.onSuccess);
        </body>
    </javascript-method>
</javascript-class>
