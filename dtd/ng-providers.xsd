<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="ng-providers">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="ng-provider"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ng-provider">
        <xs:complexType>
            <xs:attributeGroup ref="attlist.ng-provider"/>
        </xs:complexType>
    </xs:element>
    
    <xs:attributeGroup name="attlist.ng-provider">
        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="location" type="xs:string"/>
    </xs:attributeGroup>
</xs:schema>
